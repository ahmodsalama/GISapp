                //    html += "<td>" + item.Duration + "</td>";
                //    html += "<td>" + item.Duration + "</td>";
                //    // and html += other fields...
                //    $("#persons tr:last").after(html);
                //});
                $('#persons').DataTable({
                    data: result,
